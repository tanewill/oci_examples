### Authentication details
export TF_VAR_tenancy_ocid=$TF_VAR_tenancy_ocid
export TF_VAR_user_ocid=$TF_VAR_user_ocid
export TF_VAR_fingerprint=$TF_VAR_fingerprint
export TF_VAR_private_key_path=$TF_VAR_private_key_path

### Region
export TF_VAR_region=us-phoenix-1

### Compartment
export TF_VAR_compartment_ocid=$C

### Public/private keys used on the instance
export TF_VAR_ssh_public_key=$(cat ~/.ssh/id_rsa.pub)
export TF_VAR_ssh_private_key=$(cat ~/.ssh/id_rsa)
## NOTE: These are not your api keys. More info on the right keys see
## https://docs.us-phoenix-1.oraclecloud.com/Content/Compute/Tasks/managingkeypairs.htm
